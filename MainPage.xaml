<?xml version="1.0" encoding="utf-8" ?>
<ContentPage
    x:Class="VendeSpil.MainPage"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml">

    <ScrollView>
        <VerticalStackLayout Padding="30,0" Spacing="25">
            <Button
                Clicked="OnFarClicked"
                HorizontalOptions="Fill"
                Text="Far" />
            <Button
                Clicked="OnOliviaClicked"
                HorizontalOptions="Fill"
                Text="Olivia" />
            <Button
                Clicked="OnJuliusClicked"
                HorizontalOptions="Fill"
                Text="Julius" />
            <Button
                Clicked="OnAlbertClicked"
                HorizontalOptions="Fill"
                Text="Albert" />
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
